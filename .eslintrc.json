{
  "rules": {
    "no-undef": 2,
    "comma-dangle": [
      1,
      "always-multiline"
    ],
    "no-multi-str": 0,
    "no-multi-spaces": 1,
    "quotes": [
      1,
      "single"
    ],
    "no-unused-vars": [
      1,
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "curly": [
      1,
      "multi-line"
    ],
    "camelcase": [
      1,
      {
        "properties": "never"
      }
    ],
    "block-scoped-var": 1,
    "eqeqeq": [
      2,
      "smart"
    ],
    "dot-notation": 1,
    "no-else-return": 1,
    "key-spacing": [
      1,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-infix-ops": 1,
    "space-after-keywords": 1,
    "space-unary-ops": 1,
    "space-return-throw-case": 1,
    "no-process-env": 1,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 1,
    "consistent-this": [
      1,
      "self"
    ],
    "eol-last": 1,
    "no-trailing-spaces": 1,
    "new-cap": 1,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-nested-ternary": 1,
    "no-multiple-empty-lines": 1,
    "handle-callback-err": 2,
    "no-var": 1,
    "no-shadow": 0,
    "max-len": [
      1,
      100,
      2
    ],
    "no-plusplus": 1,
    "no-unused-expressions": 0,
    "prefer-const": 0,
    "object-curly-spacing": [
      1,
      "always"
    ],
    "array-bracket-spacing": [
      0,
      "never",
      {
        "singleValue": true
      }
    ],
    "computed-property-spacing": [
      1,
      "never"
    ],
    "accessor-pairs": 1,
    "spaced-comment": [
      1,
      "always"
    ],
    "no-unexpected-multiline": 2,
    "constructor-super": 2,
    "no-underscore-dangle": 0,
    "strict": [
      2,
      "global"
    ],
    "semi": [
      0,
      "always"
    ],
    "no-redeclare": [
      2,
      {
        "builtinGlobals": true
      }
    ],
    "default-case": 1,
    "no-case-declarations": 1,
    "no-duplicate-case": 1,
    "no-fallthrough": 2,
    "global-require": 2,
    "callback-return": 2,
    "no-new-require": 1,
    "no-sync": 1,
    "arrow-body-style": [
      1,
      "as-needed"
    ],
    "arrow-spacing": 1,
    "no-arrow-condition": 1,
    "require-yield": 1,
    "no-useless-concat": 1,
    "prefer-reflect": 0,
    "no-cond-assign": 1,
    "no-console": 1,
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty-character-class": 1,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-func-assign": 1,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "use-isnan": 2,
    "valid-typeof": 1,
    "consistent-return": 1,
    "guard-for-in": 1,
    "no-caller": 2,
    "no-div-regex": 1,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extra-bind": 1,
    "no-floating-decimal": 1,
    "no-implicit-coercion": [
      1,
      {
        "boolean": false,
        "number": false,
        "string": true
      }
    ],
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 0,
    "no-magic-numbers": 0,
    "no-native-reassign": 2,
    "no-new-func": 1,
    "no-new-wrappers": 2,
    "no-octal": 1,
    "no-proto": 2,
    "no-throw-literal": 2,
    "no-void": 1,
    "no-with": 2,
    "radix": [
      1,
      "as-needed"
    ],
    "no-catch-shadow": 1,
    "no-delete-var": 2,
    "no-undef-init": 1,
    "indent": [
      1,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-array-constructor": 2
  }
}
